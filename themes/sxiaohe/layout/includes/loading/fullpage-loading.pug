  }
  /* 加载动画主体 */
  .loading-bg {
    flex-direction: column!important;
    align-items: center!important;
  }
  .loading-body-right-bar{
    position: relative;
    width: 100%;
    height: 6px;
    border-left: 6px solid #a4a4a4;
    border-right: 6px solid #a4a4a4;
    display: flex;
    align-items: center;
  }
  .loading-body-right-bar-2 {
    position: absolute;
    height: 100%;
    left: 0;
    top: 0;
    background-color: #18d1ff;
    transition: progress auto ease-in-out;
  }
  ._canvas-loading {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    object-fit: cover;
    object-position: center;
  }
  @keyframes progress {
    0% {
      width: 0;
    }
    50% {
      width: 50%;
    }
    100% {
      width: 100%;
    }
  }


