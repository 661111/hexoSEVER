- const { darkmode } = theme
nav#nav
  span#blog_name
    .back-home-button(tabindex='-1')
      i.back-home-button-icon.fas.fa-grip-vertical
      .back-menu-list-groups
        .back-menu-list-group
          .back-menu-list-title 网页
          .back-menu-list
            a.back-menu-item(href='/', title='前往博客主页', target='_blank', one-link-mark='yes')
              img.back-menu-item-icon(src='/img/avatar.avif' alt="博客图标")
              span.back-menu-item-text 博客
          .back-menu-list-title 项目
          .back-menu-list
            a.back-menu-item(href='https://cloud.sxiaohe.top/', rel='external nofollow', title='前往云盘主页', target='_blank', one-link-mark='yes')
              img.back-menu-item-icon(src='/img/avatar.avif' alt="云盘图标")
              span.back-menu-item-text 晓河云盘
            a.back-menu-item(href='https://jsd.sxiaohe.top/', rel='external nofollow', title='前往云盘主页', target='_blank', one-link-mark='yes')
              img.back-menu-item-icon(src='/img/avatar.avif' alt="jsd镜像图标")
              span.back-menu-item-text jsd镜像网站

    a#site-name(href=url_for('/'))
      .title #[=config.title]
      i.fa-solid.fa-house

  #menus
    !=partial('includes/header/menu_item', {}, {cache: true})

    if (theme.algolia_search.enable || theme.local_search.enable)
      .nav-button#search-button
        a.site-page.social-icon.search
          i.fas.fa-search.fa-fw
      
      .nav-button#search-button
        a.nav-rightbutton.site-page.darkmode_switchbutton(onclick="switchDarkMode()")
          i.fas.fa-adjust
      
      .nav-button#search-button(title="随机前往一个开往项目网站")
        a.site-page(data-pjax-state="external" draggable="false" href="https://www.travellings.cn/go.html" rel="external nofollow" title="随机前往一个开往项目网站")
          i.anzhiyu-icon-train.anzhiyufont

      //- 美化设置
      .nav-button#search-button
        a.site-page.social-icon.search(onclick="toggleWinbox()" title="美化设置-自定义你的风格" id="meihua-button" rel="noopener external nofollow")
          i.fas.fa-images.fa-fw
      if theme.centerConsole.enable
        label.widget(for="center-console" title=_p("中控台") onclick="anzhiyu.switchConsole();")
          style.
            label.widget {
              --icon-size: 1.375rem;
              position: relative;
              right: 0;
              top: 0;
              bottom: 0;
              height: var(--icon-size);
              width: var(--icon-size);
              cursor: pointer;
              -webkit-transition: 1s;
              -moz-transition: 1s;
              -o-transition: 1s;
              -ms-transition: 1s;
              transition: 1s;
              margin-left: 1rem;
            }
            i.left {
              background: var(--font-color);
            }
            i.left {
              background: var(--light-grey);
              position: absolute;
              border-radius: calc(var(--icon-size)* .15);
              -webkit-transition: .5s var(--animation-on);
              -moz-transition: .5s var(--animation-on);
              -o-transition: .5s var(--animation-on);
              -ms-transition: .5s var(--animation-on);
              transition: .5s var(--animation-on);
              inset: 0;
              margin: auto;
              right: auto;
              width: calc(var(--icon-size) / 3);
              height: calc(var(--icon-size) / 3);
              -webkit-transform: translateY(calc(var(--icon-size) / 4));
              -moz-transform: translateY(calc(var(--icon-size) / 4));
              -o-transform: translateY(calc(var(--icon-size) / 4));
              -ms-transform: translateY(calc(var(--icon-size) / 4));
              transform: translateY(calc(var(--icon-size) / 4));
            }
            i.widget.center {
              background: var(--font-color);
            }
            i.widget.center {
              background: var(--light-grey);
              position: absolute;
              border-radius: calc(var(--icon-size)* .15);
              -webkit-transition: .5s var(--animation-on);
              -moz-transition: .5s var(--animation-on);
              -o-transition: .5s var(--animation-on);
              -ms-transition: .5s var(--animation-on);
              transition: .5s var(--animation-on);
              inset: 0;
              margin: auto;
              right: auto;
              width: calc(var(--icon-size) / 3);
              height: calc(var(--icon-size) / 3);
              -webkit-transform: translateY(calc(var(--icon-size) / 4));
              -moz-transform: translateY(calc(var(--icon-size) / 4));
              -o-transform: translateY(calc(var(--icon-size) / 4));
              -ms-transform: translateY(calc(var(--icon-size) / 4));
              transform: translateY(calc(var(--icon-size) / 4));
            }
            i.widget.right {
              background: var(--font-color);
            }
            i.widget.right {
              background: var(--light-grey);
              position: absolute;
              border-radius: calc(var(--icon-size)* .15);
              -webkit-transition: .5s var(--animation-on);
              -moz-transition: .5s var(--animation-on);
              -o-transition: .5s var(--animation-on);
              -ms-transition: .5s var(--animation-on);
              transition: .5s var(--animation-on);
              inset: 0;
              margin: auto;
              right: auto;
              width: calc(var(--icon-size) / 3);
              height: calc(var(--icon-size) / 3);
              -webkit-transform: translateY(calc(var(--icon-size) / 4));
              -moz-transform: translateY(calc(var(--icon-size) / 4));
              -o-transform: translateY(calc(var(--icon-size) / 4));
              -ms-transform: translateY(calc(var(--icon-size) / 4));
              transform: translateY(calc(var(--icon-size) / 4));
            }
          i.left
          i.widget.center
          i.widget.right
      
      !=partial('./console.pug', {}, {cache:true})


    .nav-button#toggle-menu
      a.site-page
        i.fas.fa-bars.fa-fw